{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks"
    ],
    "ignorePatterns": [
        "*.test.js"
    ],
    "rules": {},
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}